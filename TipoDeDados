package FiapOnJava;

public class TipoDeDados {

	public static void main(String[] args) {
		
		int idade = 10;
		double preco = 10.0;
		char genero = 'M';
		boolean maioridade = false;
		
		System.out.println(idade);	
		System.out.println(preco);
		System.out.println(genero);
		System.out.println(maioridade);
		
	}

}
